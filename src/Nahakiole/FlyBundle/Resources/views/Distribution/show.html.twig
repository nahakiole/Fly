{% extends 'FlyBundle:Default:index.html.twig' %}

{% block content -%}
    <h1>Distribution</h1>

    <table class="record_properties table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Packetmanager</th>
                <td>{{ entity.packetManager }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('distribution') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('distribution_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
